{
    "manifest_version": 2,
    "name": "Text Extractor",
    "version": "1.0",
    "description": "Extracts text from web pages",
    "permissions": ["activeTab", "clipboardWrite"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "images/icon.png"
    },
    "icons": {
      "128": "images/icon.png"
    }
  }